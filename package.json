{
	"name": "better-vue3-admin",
	"private": true,
	"version": "1.0.1",
	"type": "module",
	"description": "一个基于Vue3、Vite3、TypeScript、Element Plus 的高颜值、清新优雅、可扩展的中后台模板。 A high appearance level, fresh and elegant, scalable middle and background template based on Vue3, Vite3, TypeScript, Element Plus.",
	"license": "MIT",
	"homepage": "https://github.com/Liangjuen/better-vue3-admin",
	"repository": {
		"url": "https://github.com/Liangjuen/better-vue3-admin.git"
	},
	"keywords": [
		"Vue3 admin",
		"Vite",
		"Element Plus",
		"Vue3 admin ts",
		"TypeScript",
		"vue-admin-template"
	],
	"scripts": {
		"dev": "vite --mode dev",
		"dev:test": "vite --mode test",
		"dev:prod": "vite --mode prod",
		"build": "vue-tsc && vite build --mode prod",
		"build:test": "vue-tsc && vite build --mode test",
		"lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
		"format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
		"commit:comment": "引导设置规范化的提交信息",
		"commit": "git-cz",
		"prepare": "husky"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		},
		"cz-customizable": {
			"config": "./.cz-config.cjs"
		}
	},
	"lint-staged": {
		"*.{js,ts,vue}": [
			"npm run lint",
			"npm run format"
		]
	},
	"dependencies": {
		"@vue/shared": "^3.5.13",
		"@vueuse/core": "^12.4.0",
		"axios": "^1.6.4",
		"echarts": "^5.5.1",
		"element-plus": "^2.4.4",
		"mitt": "^3.0.1",
		"mockjs": "^1.1.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.1.7",
		"pinia-plugin-persistedstate": "^3.2.1",
		"vue": "^3.3.11",
		"vue-draggable-plus": "^0.3.5",
		"vue-router": "^4.2.5",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@types/mockjs": "^1.0.10",
		"@types/node": "^20.10.5",
		"@types/nprogress": "^0.2.3",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"@vitejs/plugin-vue": "^4.5.2",
		"commitizen": "^4.3.0",
		"commitlint-config-cz": "^0.13.3",
		"cz-conventional-changelog": "^3.3.0",
		"cz-customizable": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.1",
		"eslint-plugin-vue": "^9.19.2",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.4",
		"prettier": "^3.1.1",
		"rollup-plugin-visualizer": "^5.12.0",
		"sass": "^1.69.5",
		"typescript": "^5.2.2",
		"unplugin-auto-import": "^0.17.2",
		"unplugin-vue-components": "^0.26.0",
		"vite": "^5.0.8",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-svg-icons": "^2.0.1",
		"vue-tsc": "^1.8.25"
	}
}
